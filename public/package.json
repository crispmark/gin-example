    "@types/node": "^9.4.6",
    "@types/webpack": "^3.8.8",
    "@vue/test-utils": "^1.0.0-beta.12",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "file-loader": "^1.1.11",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.0.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^5.1.0",
